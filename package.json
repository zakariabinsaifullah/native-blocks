{
    "name": "bem-blocks",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "wp-scripts build",
        "start": "wp-scripts start",
        "packages-update": "wp-scripts packages-update",
        "make-pot": "wp i18n make-pot . languages/bem-blocks.pot --exclude=node_modules,assets,build",
        "zip-plugin": "cd .. && zip -r bem-blocks.zip bem-blocks -x \"bem-blocks/node_modules/*\" \"bem-blocks/src/*\" \"bem-blocks/.git/*\" \"bem-blocks/.vscode/*\" \"bem-blocks/.DS_Store\" \"bem-blocks/**/.DS_Store/*\" \"bem-blocks/.idea/*\" \"bem-blocks/phpcs.xml\" \"bem-blocks/composer.lock\" \"bem-blocks/.eslintrc.json\" \"bem-blocks/package.json\" \"bem-blocks/package-lock.json\" \"bem-blocks/webpack.config.js\" \"bem-blocks/.gitignore\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "htmlWhitespaceSensitivity": "css",
        "insertPragma": false,
        "jsxBracketSameLine": false,
        "jsxSingleQuote": false,
        "printWidth": 140,
        "proseWrap": "preserve",
        "quoteProps": "as-needed",
        "requirePragma": false,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "none",
        "useTabs": false,
        "rangeStart": 0
    },
    "devDependencies": {
        "@wordpress/scripts": "^30.17.0"
    },
    "dependencies": {
        "classnames": "^2.5.1"
    }
}
